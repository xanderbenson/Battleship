<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="battleshipgui.MainController">
   <children>
      <Label alignment="CENTER" layoutX="79.0" layoutY="14.0" text="Welcome To Battleship">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <VBox alignment="CENTER" layoutX="123.0" layoutY="120.0" prefHeight="260.0" prefWidth="255.0">
         <children>
            <HBox fx:id="newgamebox" alignment="CENTER" prefHeight="80.0" prefWidth="255.0">
               <children>
                  <Button fx:id="Start" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#handleStart" text="New Game">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#handleLoad" text="Load Game">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handleInfo" text="Instructions" />
         </children>
      </VBox>
   </children>
</AnchorPane>
